<?xml version="1.0" encoding="UTF-8"?>
<sitemesh>
	<!-- 指明满足“/*”的页面，将被“/WEB-INF/views/decorators/decorator.html”所装饰 -->
	<!-- 默认装饰器，当下面的路径都不匹配时，启用该装饰器进行装饰 -->
	<mapping path="/modeler.html" exclue="true" />
	<mapping path="/diagram-viewer/*" exclue="true" />
	<mapping path="/editor-app/*" exclue="true" />
	<mapping path="/*" decorator="/common/header.jsp" />

    <!-- 指明满足“/exclude.jsp*”的页面，将被排除，不被装饰 -->
	<mapping path="/login" exclue="true" />

</sitemesh>